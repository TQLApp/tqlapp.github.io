{
  "rules": {
    "InvalidAssemblyReference": "info"
  },
  "build": {
    "content": [
      {
        "files": ["**/*.{md,yml}"],
        "exclude": ["_site/**"]
      }
    ],
    "resource": [
      {
        "files": ["Images/**"]
      }
    ],
    "output": "_site",
    "template": ["default", "modern", "Template"],
    "postProcessors": ["FixImageTags"],
    "globalMetadata": {
      "_appName": "Techie's Quick Launcher",
      "_appTitle": "Techie's Quick Launcher",
      "_appLogoPath": "Images/Logo.svg",
      "_appFaviconPath": "Images/Logo.png",
      "_appFooter": "<span>Made with <a href=\"https://dotnet.github.io/docfx\">docfx</a> | <a href=\"/Privacy-policy.html\">Privacy Policy</a></span>",
      "_disableContribution": true,
      "_enableSearch": false,
      "pdf": false
    }
  }
}
